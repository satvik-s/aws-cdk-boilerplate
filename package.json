{
  "name": "hello-world-lambda",
  "version": "0.1.0",
  "bin": {
    "hello-world-lambda": "bin/hello-world-lambda.js"
  },
  "scripts": {
    "build": "$(npm bin)/tsc",
    "watch": "$(npm bin)/tsc -w",
    "cdk": "cdk",
    "static-analysis": "$(npm bin)/eslint .",
    "lint-fix": "$(npm bin)/eslint --fix .",
    "prettier": "$(npm bin)/prettier --write .",
    "check-updates": "$(npm bin)/ncu",
    "make-updates": "$(npm bin)/ncu -u",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/node": "20.1.7",
    "@typescript-eslint/eslint-plugin": "5.59.6",
    "@typescript-eslint/parser": "5.59.6",
    "aws-cdk-lib": "2.79.1",
    "constructs": "10.2.17",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "npm-check-updates": "16.10.12",
    "prettier": "2.8.8",
    "source-map-support": "0.5.21",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  },
  "lint-staged": {
    "*.{js,ts,json}": "npm run prettier"
  }
}
