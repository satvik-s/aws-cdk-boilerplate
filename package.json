{
  "name": "hello-world-lambda",
  "version": "0.1.0",
  "bin": {
    "hello-world-lambda": "bin/hello-world-lambda.js"
  },
  "scripts": {
    "build": "$(npm bin)/tsc",
    "watch": "$(npm bin)/tsc -w",
    "cdk": "cdk",
    "static-analysis": "$(npm bin)/eslint .",
    "lint-fix": "$(npm bin)/eslint --fix .",
    "prettier": "$(npm bin)/prettier --write .",
    "check-updates": "$(npm bin)/ncu",
    "make-updates": "$(npm bin)/ncu -u",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/node": "20.8.4",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "aws-cdk-lib": "2.100.0",
    "constructs": "10.3.0",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "npm-check-updates": "16.14.6",
    "prettier": "3.0.3",
    "source-map-support": "0.5.21",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "lint-staged": {
    "*.{js,ts,json}": "npm run prettier"
  }
}
