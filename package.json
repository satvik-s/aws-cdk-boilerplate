{
  "name": "hello-world-lambda",
  "version": "0.1.0",
  "bin": {
    "hello-world-lambda": "bin/hello-world-lambda.js"
  },
  "scripts": {
    "build": "$(npm bin)/tsc",
    "watch": "$(npm bin)/tsc -w",
    "cdk": "cdk",
    "static-analysis": "$(npm bin)/eslint .",
    "lint-fix": "$(npm bin)/eslint --fix .",
    "prettier": "$(npm bin)/prettier --write .",
    "check-updates": "$(npm bin)/ncu",
    "make-updates": "$(npm bin)/ncu -u",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/node": "20.12.5",
    "@typescript-eslint/eslint-plugin": "7.5.0",
    "@typescript-eslint/parser": "7.5.0",
    "aws-cdk-lib": "2.135.0",
    "constructs": "10.3.0",
    "eslint": "9.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-simple-import-sort": "12.0.0",
    "eslint-plugin-unused-imports": "3.1.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "npm-check-updates": "16.14.18",
    "prettier": "3.2.5",
    "source-map-support": "0.5.21",
    "ts-node": "10.9.2",
    "typescript": "5.4.4"
  },
  "lint-staged": {
    "*.{js,ts,json}": "npm run prettier"
  }
}
